									"assetType": {
										"type": "string",
										"enum": [
											"NATIVE",
											"ERC20",
											"ERC721",
											"ERC1155",
											"SPECIAL_NFT"
										]
									},
									"changeType": {
										"type": "string",
										"enum": [
											"APPROVE",
											"TRANSFER"
										]
									},
									"from": {
										"title": "hex encoded address",
										"type": "string",
										"pattern": "^0x[0-9a-fA-F]{40}$",
										"description": "address the transaction is sent from"
									},
									"to": {
										"title": "hex encoded address",
