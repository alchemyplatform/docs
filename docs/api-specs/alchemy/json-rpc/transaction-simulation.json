										"pattern": "^0x[0-9a-fA-F]{40}$",
										"description": "address the transaction is directed to"
									},
									"rawAmount": {
										"type": "string"
									},
									"amount": {
										"type": "string"
									},
									"symbol": {
										"type": "string"
									},
									"decimals": {
										"type": [
											"number",
											"null"
										]
									},
									"contractAddress": {
										"type": [
											"string",
											"null"
										]
									},
									"name": {
										"type": [
											"string",
											"null"
										]
									},
									"logo": {
										"type": [
											"string",
											"null"
										]
									},
									"tokenId": {
										"type": [
											"number",
											"null"
										]
									}
								}
							}
						},
						"gasUsed": {
							"type": "string",
							"description": "The amount of gas used for the simulation"
