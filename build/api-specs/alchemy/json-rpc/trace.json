    {
      "name": "trace_replayBlockTransactions",
      "summary": "Replays all transactions in a block, returning the requested traces for each transaction.",
      "params": [
        {
          "name": "blockIdentifier",
          "required": true,
          "description": "Block identifier as a string. This can be a block hash, a block number (in hex), or a block tag (such as \"latest\", \"finalized\", etc.).\n",
